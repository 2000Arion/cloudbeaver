{
  "name": "@dbeaver/dbeaver",
  "version": "0.1.0",
  "description": "",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js --mode=production",
    "build-exp": "npm run clean-dist && webpack -p --config=configs/webpack/exp.js --mode=development",
    "build-exp2": "npm run clean-dist && builder build-app",
    "build:analyze": "echo \"no analyze specified\" && exit 0",
    "clean-dist": "rimraf dist/*",
    "dev": "webpack-dev-server --progress --config=configs/webpack/dev.js --port=3100",
    "dev-exp": "webpack-dev-server --progress --config=configs/webpack/dev-exp.js --port=3100",
    "dev-exp2": "builder serve-app",
    "dev-exp3": "builder serve-app-old",
    "gql:gen": "graphql-codegen",
    "gql:gen:dev": "graphql-codegen --watch",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "lint-fix": "eslint ./src/ --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@dbeaver/administration": "^0.1.0",
    "@dbeaver/authentication": "^0.1.0",
    "@dbeaver/ag-grid-plugin": "^0.1.0",
    "@dbeaver/basic-connection-plugin": "^0.1.0",
    "@dbeaver/builder": "^0.1.0",
    "@dbeaver/core": "^0.1.0",
    "@dbeaver/custom-connection-plugin": "^0.1.0",
    "@dbeaver/data-viewer-plugin": "^0.1.0",
    "@dbeaver/ddl-viewer-plugin": "^0.1.0",
    "@dbeaver/object-viewer-plugin": "^0.1.0",
    "@dbeaver/sql-editor": "^0.1.0",
    "@dbeaver/data-export-plugin": "^0.1.0"
  },
  "buildVersion": "0.13.0"
}
